{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "ConfigureArg": {
      "title": "ConfigureArg",
      "type": "object",
      "required": [
        "lang",
        "trace"
      ],
      "properties": {
        "lang": {
          "$ref": "#/definitions/Lang"
        },
        "trace": {
          "$ref": "#/definitions/CoreTrace"
        }
      }
    },
    "CoreTrace": {
      "description": "documentation",
      "type": "object",
      "required": [
        "binary",
        "callArgsEnabled",
        "calltrace",
        "debug",
        "eventsEnabled",
        "historyEnabled",
        "imported",
        "paths",
        "preloadEnabled",
        "program",
        "replay",
        "telemetry",
        "test",
        "traceEnabled",
        "traceId",
        "traceOutputFolder"
      ],
      "properties": {
        "binary": {
          "type": "string"
        },
        "callArgsEnabled": {
          "type": "boolean"
        },
        "calltrace": {
          "type": "boolean"
        },
        "debug": {
          "type": "boolean"
        },
        "eventsEnabled": {
          "type": "boolean"
        },
        "historyEnabled": {
          "type": "boolean"
        },
        "imported": {
          "type": "boolean"
        },
        "paths": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "preloadEnabled": {
          "type": "boolean"
        },
        "program": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "replay": {
          "type": "boolean"
        },
        "telemetry": {
          "type": "boolean"
        },
        "test": {
          "type": "boolean"
        },
        "traceEnabled": {
          "type": "boolean"
        },
        "traceId": {
          "type": "integer",
          "format": "int64"
        },
        "traceOutputFolder": {
          "type": "string"
        }
      }
    },
    "CtLoadLocalsArguments": {
      "title": "CtLoadLocalsArguments",
      "description": "args for `ct/load-locals`",
      "type": "object",
      "required": [
        "countBudget",
        "minCountLimit",
        "rrTicks"
      ],
      "properties": {
        "countBudget": {
          "type": "integer",
          "format": "int64"
        },
        "minCountLimit": {
          "type": "integer",
          "format": "int64"
        },
        "rrTicks": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "CtLoadLocalsResponseBody": {
      "title": "CtLoadLocalsResponseBody",
      "description": "response for `ct/load-locals`",
      "type": "object",
      "required": [
        "locals"
      ],
      "properties": {
        "locals": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Variable"
          }
        }
      }
    },
    "Lang": {
      "type": "string",
      "enum": [
        "C",
        "Cpp",
        "Rust",
        "Nim",
        "Go",
        "Pascal",
        "Python",
        "Ruby",
        "RubyDb",
        "Javascript",
        "Lua",
        "Asm",
        "Noir",
        "RustWasm",
        "CppWasm",
        "Small",
        "Unknown"
      ]
    },
    "Type": {
      "type": "object",
      "properties": {
        "cType": {
          "default": "",
          "type": "string"
        },
        "elementType": {
          "default": null,
          "anyOf": [
            {
              "$ref": "#/definitions/Type"
            },
            {
              "type": "null"
            }
          ]
        },
        "kind": {
          "default": 0,
          "allOf": [
            {
              "$ref": "#/definitions/TypeKind"
            }
          ]
        },
        "labels": {
          "default": [],
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "langType": {
          "default": "",
          "type": "string"
        },
        "memberTypes": {
          "default": [],
          "type": "array",
          "items": {
            "$ref": "#/definitions/Type"
          }
        }
      }
    },
    "TypeKind": {
      "description": "Categories of types recorded in the trace.",
      "type": "string",
      "enum": [
        "Seq",
        "Set",
        "HashSet",
        "OrderedSet",
        "Array",
        "Varargs",
        "Struct",
        "Int",
        "Float",
        "String",
        "CString",
        "Char",
        "Bool",
        "Literal",
        "Ref",
        "Recursion",
        "Raw",
        "Enum",
        "Enum16",
        "Enum32",
        "C",
        "TableKind",
        "Union",
        "Pointer",
        "Error",
        "FunctionKind",
        "TypeValue",
        "Tuple",
        "Variant",
        "Html",
        "None",
        "NonExpanded",
        "Any",
        "Slice"
      ]
    },
    "Value": {
      "type": "object",
      "properties": {
        "address": {
          "default": "",
          "type": "string"
        },
        "b": {
          "default": false,
          "type": "boolean"
        },
        "c": {
          "default": "",
          "type": "string"
        },
        "cText": {
          "default": "",
          "type": "string"
        },
        "elements": {
          "default": [],
          "type": "array",
          "items": {
            "$ref": "#/definitions/Value"
          }
        },
        "f": {
          "default": "",
          "type": "string"
        },
        "i": {
          "default": "",
          "type": "string"
        },
        "isMutable": {
          "default": false,
          "type": "boolean"
        },
        "kind": {
          "default": 0,
          "allOf": [
            {
              "$ref": "#/definitions/TypeKind"
            }
          ]
        },
        "msg": {
          "default": "",
          "type": "string"
        },
        "r": {
          "default": "",
          "type": "string"
        },
        "refValue": {
          "default": null,
          "anyOf": [
            {
              "$ref": "#/definitions/Value"
            },
            {
              "type": "null"
            }
          ]
        },
        "text": {
          "default": "",
          "type": "string"
        },
        "typ": {
          "default": {
            "cType": "",
            "elementType": null,
            "kind": 0,
            "labels": [],
            "langType": "",
            "memberTypes": []
          },
          "allOf": [
            {
              "$ref": "#/definitions/Type"
            }
          ]
        }
      }
    },
    "Variable": {
      "type": "object",
      "required": [
        "expression",
        "value"
      ],
      "properties": {
        "expression": {
          "type": "string"
        },
        "value": {
          "$ref": "#/definitions/Value"
        }
      }
    }
  }
}
