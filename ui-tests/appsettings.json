{
  "Runner": {
    "MaxParallelInstances": null,
    "StopOnFirstFailure": false,
    "IncludeTests": [],
    "ExcludeTests": [],
    "DefaultMode": "Electron",
    "ExecutionModes": [
      "Electron",
      "Web"
    ],
    "Scheduler": "Semaphore",
    "DiagnosticsOutput": null
  },
  "Electron": {
    "TraceProgram": "noir_space_ship",
    "CdpStartupTimeoutSeconds": 25,
    "SharedMemoryDirectory": null
  },
  "Web": {
    "HostStartupTimeoutSeconds": 45,
    "BrowserWindow": {
      "Headless": false,
      "WindowSize": "1920x1080",
      "PreferredDisplay": null
    },
    "Ports": {
      "PortStrategy": "Random",
      "FixedPort": null,
      "PreferredPorts": [
        5300,
        5301
      ]
    }
  },
  "Scenarios": [
    {
      "Id": "noir-electron-default",
      "Description": "Electron smoke scenario for Noir Space Ship timeline navigation.",
      "Mode": "Electron",
      "Test": "NoirSpaceShip.JumpToAllEvents",
      "Enabled": true,
      "EventIndex": 3,
      "DelaySeconds": 6,
      "Tags": [
        "smoke",
        "electron"
      ],
      "CollectTelemetry": false
    },
    {
      "Id": "noir-web-default",
      "Description": "Web smoke scenario for Noir Space Ship timeline navigation.",
      "Mode": "Web",
      "Test": "NoirSpaceShip.JumpToAllEvents",
      "Enabled": true,
      "EventIndex": 3,
      "DelaySeconds": 6,
      "Tags": [
        "smoke",
        "web"
      ],
      "CollectTelemetry": false
    },
    {
      "Id": "noir-electron-create-trace",
      "Description": "Electron coverage for trace creation example.",
      "Mode": "Electron",
      "Test": "NoirSpaceShip.CreateSimpleTracePoint",
      "Enabled": true,
      "EventIndex": 3,
      "DelaySeconds": 2,
      "Tags": [
        "trace",
        "electron"
      ],
      "CollectTelemetry": true
    },
    {
      "Id": "noir-web-editor",
      "Description": "Web coverage for verifying Noir editor state.",
      "Mode": "Web",
      "Test": "NoirSpaceShip.EditorLoadedMainNrFile",
      "Enabled": true,
      "EventIndex": 3,
      "DelaySeconds": 4,
      "Tags": [
        "editor",
        "web"
      ],
      "CollectTelemetry": false
    }
  ]
}
